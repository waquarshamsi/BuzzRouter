# This file is an example. Copy it to .env and fill in your production secrets.
# The .env file should be added to .gitignore and never committed to version control.

# Spring Boot Application Configuration
SPRING_PROFILES_ACTIVE=prod

# RabbitMQ Credentials
RABBITMQ_USER=prod_user
RABBITMQ_PASSWORD=StrongPassword123!

# Elasticsearch Credentials
ELASTIC_PASSWORD=StrongElasticPassword123!

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=YOUR_PRODUCTION_TELEGRAM_BOT_TOKEN

# Email (SMTP) Credentials
MAIL_HOST=smtp.prod-mail-server.com
MAIL_PORT=587
MAIL_USERNAME=prod_smtp_user
MAIL_PASSWORD=StrongSmtpPassword123!
MAIL_FROM=noreply@your-prod-domain.com

# Twilio Credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=YOUR_PRODUCTION_TWILIO_AUTH_TOKEN
TWILIO_FROM_NUMBER=+15551234567